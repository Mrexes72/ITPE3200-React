  "version": "0.2.0",
  "configurations": [
    {
      "name": ".Net Core Launch (web)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/api/bin/Debug/net8.0/api.dll",
      "args": [],
      "cwd": "${workspaceFolder}/api",
      "stopAtEntry": false,
      "launchBrowser": {
        "enabled": true
